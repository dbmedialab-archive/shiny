{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react",
		["@babel/preset-stage-0", { "decoratorsLegacy": true }]
	],
	"plugins": [
		"@babel/plugin-transform-object-assign",
		"react-docgen",
		[
			"styled-components",
			{
				"ssr": true,
				"displayName": true,
				"preprocess": false
			}
		],
		[
			"transform-imports",
			{
				"..": {
					"transform": "./../../babel-tranform-shiny.js",
					"preventFullImport": true,
					"skipDefaultConversion": true
				}
			}
		]
	]
}
