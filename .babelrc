{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react",
		"@babel/preset-stage-0"
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-transform-object-assign",
		[
			"styled-components",
			{
				"ssr": true,
				"displayName": true,
				"preprocess": false
			}
		]
	]
}
